<?xml version="1.0" encoding="UTF-8"?>
<ConfigChunkDB>
  <InputManager name="Input Manager">
    <driver>${VJ_BASE_DIR}/lib/gadgeteer/drivers/VRPN_drv.so</driver>
  </InputManager>
  <proxyAlias name="VJHead">
    <aliasName>VJHead</aliasName>
    <proxyPtr>HeadProxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJWand">
    <aliasName>VJWand</aliasName>
    <proxyPtr>WandProxy</proxyPtr>
  </proxyAlias>
  <PosProxy name="HeadProxy">
    <device>Vrpn</device>
    <unit>1</unit>
    <position_filters>
      <PosXformFilter name="Position Filters">
        <pre_translate>0.0</pre_translate>
        <pre_translate>0.0</pre_translate>
        <pre_translate>0.0</pre_translate>
        <pre_rotation>0.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <custom_scale>1.0</custom_scale>
        <dev_units>1.0</dev_units>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_rotation>0.0</post_rotation>
        <post_rotation>90.0</post_rotation>
        <post_rotation>-90.0</post_rotation>
      </PosXformFilter>
    </position_filters>
  </PosProxy>
  <PosProxy name="WandProxy">
    <device>Vrpn</device>
    <unit>0</unit>
    <position_filters>
      <PosXformFilter name="Position Filters">
        <pre_translate>0.0</pre_translate>
        <pre_translate>0.0</pre_translate>
        <pre_translate>0.0</pre_translate>
        <pre_rotation>0.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <custom_scale>1.0</custom_scale>
        <dev_units>1.0</dev_units>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_rotation>0.0</post_rotation>
        <post_rotation>90.0</post_rotation>
        <post_rotation>-90.0</post_rotation>
      </PosXformFilter>
    </position_filters>
  </PosProxy>
  <Vrpn name="Vrpn">
    <trackerServer>Tracker0@localhost.localdomain</trackerServer>
    <buttonServer>Button0@localhost.localdomain</buttonServer>
    <numTrackers>2</numTrackers>
    <numButtons>4</numButtons>
    <position_filters>
      <PosXformFilter name="Position Filters">
        <pre_translate>0.0</pre_translate>
        <pre_translate>0.42672</pre_translate>
        <pre_translate>0.0</pre_translate>
        <pre_rotation>90.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <pre_rotation>0.0</pre_rotation>
        <custom_scale>1.0</custom_scale>
        <dev_units>1.0</dev_units>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_translate>0.0</post_translate>
        <post_rotation>0.0</post_rotation>
        <post_rotation>0.0</post_rotation>
        <post_rotation>0.0</post_rotation>
      </PosXformFilter>
    </position_filters>
    <deviceHost />
  </Vrpn>
  <DigProxy name="Button0Proxy">
    <device>Vrpn</device>
    <unit>0</unit>
  </DigProxy>
  <DigProxy name="Button1Proxy">
    <device>Vrpn</device>
    <unit>1</unit>
  </DigProxy>
  <DigProxy name="Button2Proxy">
    <device>Vrpn</device>
    <unit>2</unit>
  </DigProxy>
  <DigProxy name="Button3Proxy">
    <device>Vrpn</device>
    <unit>3</unit>
  </DigProxy>
  <proxyAlias name="VJButton0">
    <aliasName>VJButton0</aliasName>
    <proxyPtr>Button0Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton1">
    <aliasName>VJButton1</aliasName>
    <proxyPtr>Button1Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton2">
    <aliasName>VJButton2</aliasName>
    <proxyPtr>Button2Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton3">
    <aliasName>VJButton3</aliasName>
    <proxyPtr>Button3Proxy</proxyPtr>
  </proxyAlias>
</ConfigChunkDB>

