<!--
  This fills in the heading for a detailed description of a constructor or
  method.  It provides an anchor for external linking directly to this
  entry in the detailed listing.
  This file is specific to the VR Juggler documetnation layout.
 -->

<TR>
  <TH ALIGN="LEFT" BGCOLOR="#4682b4">
    {nolink true}
    <A NAME="{memberef}">
    <FONT SIZE="+1" COLOR="#ffffff">{mname}</FONT>
    </A>
    {nolink false}
  </TH>
</TR>
