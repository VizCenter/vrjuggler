# -----------------------------------------------------------------------------
# Makefile.in for vapor/external/cppdom.  It requires GNU make.
#
# Generated for use on @PLATFORM@
# -----------------------------------------------------------------------------
default: all

# Include common definitions here.
include @topdir@/make.defs.mk

ifeq ($(OS_TYPE), Win32)
   EXTRA_DEFS+= -DWIN32
endif

# There is nothing to install from this directory.
INSTALL_FILES=

srcdir=		@UNIX_CPPDOM_ROOT@/cppdom
top_srcdir=	@top_srcdir@
SUBOBJDIR=	$(VPR_LIBRARY)/ext/cppdom

SRCS=		version.cpp xmlparser.cpp cppdom.cpp xmltokenizer.cpp

include $(MKPATH)/dpp.obj.mk

# -----------------------------------------------------------------------------
# Include dependencies generated automatically.
# -----------------------------------------------------------------------------
ifndef DO_CLEANDEPEND
   -include $(DEPEND_FILES)
endif
