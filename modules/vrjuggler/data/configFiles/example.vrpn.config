<?xml version="1.0" encoding="UTF-8"?>
<ConfigChunkDB>
  <InputManager name="Input Manager">
    <driver>${VJ_BASE_DIR}/lib/gadgeteer/drivers/VRPN_drv.so</driver>
  </InputManager>
  <proxyAlias name="VJHead">
    <aliasName>VJHead</aliasName>
    <proxyPtr>HeadProxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJWand">
    <aliasName>VJWand</aliasName>
    <proxyPtr>WandProxy</proxyPtr>
  </proxyAlias>
  <PosProxy name="HeadProxy">
    <device>Vrpn</device>
    <unit>1</unit>
    <etrans>1</etrans>
    <translate>0.0</translate>
    <translate>0.0</translate>
    <translate>0.0</translate>
    <rotate>0.0</rotate>
    <rotate>90.0</rotate>
    <rotate>-90.0</rotate>
  </PosProxy>
  <PosProxy name="WandProxy">
    <device>Vrpn</device>
    <unit>0</unit>
    <etrans>1</etrans>
    <translate>0.0</translate>
    <translate>0.0</translate>
    <translate>0.0</translate>
    <rotate>0.0</rotate>
    <rotate>90.0</rotate>
    <rotate>-90.0</rotate>
  </PosProxy>
  <Vrpn name="Vrpn">
    <trackerServer>Tracker0@alpha.cs.ucl.ac.uk</trackerServer>
    <buttonServer>Button0@alpha.cs.ucl.ac.uk</buttonServer>
    <numTrackers>2</numTrackers>
    <numButtons>4</numButtons>
    <etrans>1</etrans>
    <pretranslate>0.0</pretranslate>
    <pretranslate>1.4</pretranslate>
    <pretranslate>0.0</pretranslate>
    <prerotate>90.0</prerotate>
    <prerotate>0.0</prerotate>
    <prerotate>0.0</prerotate>
    <prescale>0.305</prescale>
    <prescale>0.305</prescale>
    <prescale>-0.305</prescale>
    <postscale>1</postscale>
    <postscale>1</postscale>
    <postscale>-1</postscale>
  </Vrpn>
  <DigProxy name="Button0Proxy">
    <device>Vrpn</device>
    <unit>0</unit>
    <location />
  </DigProxy>
  <DigProxy name="Button1Proxy">
    <device>Vrpn</device>
    <unit>1</unit>
    <location />
  </DigProxy>
  <DigProxy name="Button2Proxy">
    <device>Vrpn</device>
    <unit>2</unit>
    <location />
  </DigProxy>
  <DigProxy name="Button3Proxy">
    <device>Vrpn</device>
    <unit>3</unit>
    <location />
  </DigProxy>
  <proxyAlias name="VJButton0">
    <aliasName>VJButton0</aliasName>
    <proxyPtr>Button0Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton1">
    <aliasName>VJButton1</aliasName>
    <proxyPtr>Button1Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton2">
    <aliasName>VJButton2</aliasName>
    <proxyPtr>Button2Proxy</proxyPtr>
  </proxyAlias>
  <proxyAlias name="VJButton3">
    <aliasName>VJButton3</aliasName>
    <proxyPtr>Button3Proxy</proxyPtr>
  </proxyAlias>
</ConfigChunkDB>

