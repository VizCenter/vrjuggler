# Makefile.in for sonix/test
# Generated for use on @PLATFORM@

default: TestSuite

SUBDIRS= \
	TestSuite 

all: $(SUBDIRS)

$(SUBDIRS)::
	cd $@ && $(MAKE)

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	@for dir in $(SUBDIRS) ; do			\
            echo "==> Cleaning in $$dir"       ;	\
            cd $$dir && $(MAKE) clean && cd .. ;	\
			done
