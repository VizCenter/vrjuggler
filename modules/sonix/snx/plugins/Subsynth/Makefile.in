default: all

include @topdir@/make.defs.mk

SRCS = SubsynthSoundImplementation.cpp

# the include path directory to install to ...
includedir	= @includedir@/snx

srcdir		= @srcdir@
top_srcdir	= @top_srcdir@
INSTALL		= @INSTALL@

EXTRA_SRCS_PATH = $(srcdir)/plugins
EXTRA_DEFS	+= -DNO_SELF_REGISTER

SYN_INCLUDES	= @VPR_INCLUDES@ @SYN_INCLUDES@
EXTRA_INCLUDES	+= $(SYN_INCLUDES)

ifeq ($(OS_TYPE), Win32)
   EXTRA_DEFS+=	-DWIN32
endif

