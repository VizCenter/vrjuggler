# $Id$
# -----------------------------------------------------------------------------

# -----------------------------------------------------------------------------
# Build targets.
# -----------------------------------------------------------------------------
default: all

all: CfgTest input ogl pfNav Quat

CfgTest input ogl pfNav Quat::
	cd $@ && ${MAKE} all

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	-cd CfgTest && ${MAKE} clean
	-cd input   && ${MAKE} clean
	-cd ogl     && ${MAKE} clean
	-cd pfNav   && ${MAKE} clean
	-cd Quat    && ${MAKE} clean
