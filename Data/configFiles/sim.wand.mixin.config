SimPosition
  Name "SimWandPos"
  keyboardProxy { "WandKeyboardProxy" }
  keyPairs { 
      KeyModPair
        Name "Forward"
        modKey { ALT }
        key { MOUSE_NEGY }
        end
 
      KeyModPair
        Name "Back"
        modKey { ALT }
        key { MOUSE_POSY }
        end
 
      KeyModPair
        Name "Left"
        modKey { CTRL }
        key { MOUSE_NEGX }
        end
 
      KeyModPair
