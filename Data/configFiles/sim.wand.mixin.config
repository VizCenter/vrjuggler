        modKey { SHIFT }
        key { MOUSE_NEGY }
        end
 
      KeyModPair
        Name "Rot Down"
        modKey { SHIFT }
        key { MOUSE_POSY }
        end
 
      KeyModPair
        Name "Rot CCW"
        modKey { SHIFT }
        key { MOUSE_BUT1 }
        end
 
      KeyModPair
        Name "Rot CW"
        modKey { SHIFT }
        key { MOUSE_BUT3 }
        end
 }
