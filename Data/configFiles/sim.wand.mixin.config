        modKey { CTRL }
        key { MOUSE_POSX }
        end
 
      KeyModPair
        Name "Up"
        modKey { CTRL }
        key { MOUSE_NEGY }
        end
 
      KeyModPair
        Name "Down"
        modKey { CTRL }
        key { MOUSE_POSY }
        end
 
      KeyModPair
        Name "Rot Right"
        modKey { SHIFT }
        key { MOUSE_NEGX }
        end
 
      KeyModPair
        Name "Rot Left"
        modKey { SHIFT }
        key { MOUSE_POSX }
        end
 
      KeyModPair
