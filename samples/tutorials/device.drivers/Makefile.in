# -----------------------------------------------------------------------------
# Makefile.in for juggler/samples/tutorials/device.drivers
#
# Generated for use on @PLATFORM@
# -----------------------------------------------------------------------------

.SUFFIXES: .cpp .@OBJ_FILE_SUFFIX@

srcdir		= @srcdir@

EXTRA_FLAGS	= @APP_EXTRA_FLAGS@
COMPILE		= @CXX@ @DEFS@ $(VJ_INCLUDES) @CPPFLAGS@ @CXXFLAGS@ @DBG_FLAGS@ $(EXTRA_FLAGS)
VJ_INCLUDES	= @INCLUDES@ -I$(VJ_BASE_DIR)/include -I$(srcdir)
VPATH		= @srcdir@

OBJECTS		= buttondevice.@OBJ_FILE_SUFFIX@

# -----------------------------------------------------------------------------
# Targets.
# -----------------------------------------------------------------------------
default: all

all: $(OBJECTS)

.cpp.@OBJ_FILE_SUFFIX@:
	$(COMPILE) @OBJ_NAME_FLAG@ -c $<

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
clean:
	rm -f Makedepend buttondevice.ilk $(OBJECTS) *core	\
          so_locations *.?db
	rm -rf ./ii_files
