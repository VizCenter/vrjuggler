default: all

includedir	= @includedir@/Input/vjPosition
srcdir		= @srcdir@
topdir		= @topdir@
top_srcdir	= @top_srcdir@
wwwdir		= ${srcdir}/Doc
VJROOT		= ${top_srcdir}
VJ_SRCDIR	= @VJ_SRCDIR@/Input/vjPosition
WORKDIR		= ${topdir}/Input/vjPosition

# Include common definitions
include ${topdir}/Makefile.base
include ${MKPATH}/vj.dep.mk
include ${MKPATH}/vj.install.mk

#--- Search path -------
vpath
vpath %.c @srcdir@
vpath %.C @srcdir@
vpath %.o ${OBJDIR}
#------------------------

DEPEND_SRCS	:= vjBird.C		\
                   vjDummyPosition.C	\
                   vjFlock.C		\
                   vjPosition.C		\
                   logiclass.C

VJPOS_OBJECTS	:= $(addprefix $(OBJDIR)/,$(DEPEND_SRCS:.C=.o))

# -----------------------------------------------------------------------------
# Library targets.
# -----------------------------------------------------------------------------
all dbg opt dso ddso obj: ${VJPOS_OBJECTS}

# -----------------------------------------------------------------------------
# Clean-up.
# -----------------------------------------------------------------------------
CLEAN_FILES	+= ${VJPOS_OBJECTS}

